        function initMap() {
            var input = document.getElementById('pickupaddress');

            var autocomplete = new google.maps.places.Autocomplete(input);
            autocomplete.addListener('place_changed', function () {
                var place = autocomplete.getPlace();
                //console.log(place);
                var lat = place.geometry.location.lat();
                var long = place.geometry.location.lng();
                var jaddress = place.formatted_address;
                console.log("lat and long" + lat, long);
                var scope = angular.element(document.getElementById('pickupaddress')).scope();
                scope.pickup.Address = jaddress;
                scope.pickup.lat = lat;
                scope.pickup.long = long;
            })

        }
    </script>
